<Page
    x:Class="TracyTest.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:TracyTest"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <StackPanel
        HorizontalAlignment="Center"
        VerticalAlignment="Center"
        Orientation="Horizontal">
        <Button x:Name="zoneScopedButton" Click="ZoneScopedClickHandler">ZoneScoped</Button>
        <Button x:Name="zoneScopedNButton" Click="ZoneScopedNClickHandler">ZoneScopedN</Button>
        <Button x:Name="frameMarkButton" Click="FrameMarkClickHandler">FrameMark</Button>
    </StackPanel>
</Page>
